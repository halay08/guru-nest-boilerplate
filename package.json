{
    "name": "guru-nestjs-boilerplate",
    "version": "0.0.1",
    "description": "NestJS Boilerplate, Typescript, MySQL, TypeORM",
    "author": "Khiem Le <halay08@gmail.com>",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.build.json",
        "postbuild": "copyfiles --up 1 src/**/*.json dist",
        "hmr": "node dist/main.hmr.ts",
        "dev": "nodemon",
        "prod": "node dist/main.js",
        "typeorm": "ts-node node_modules/typeorm/cli.js  -f ormconfig",
        "migration:generate": "yarn run typeorm migration:generate",
        "migration:create": "yarn run typeorm migration:create",
        "migration:run": "yarn run typeorm migration:run",
        "migration:revert": "yarn run typeorm migration:revert",
        "schema:drop": "yarn run typeorm schema:drop",
        "schema:sync": "yarn run typeorm schema:sync",
        "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "watch": "nodemon --config nodemon.json",
        "debug": "nodemon --config nodemon-debug.json",
        "webpack": "webpack --config webpack.config.js --progress",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docs:dev": "vuepress dev -p 7070",
        "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
        "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist"
    },
    "dependencies": {
        "@nestjs-query/core": "^0.26.0",
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/microservices": "^7.6.15",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/swagger": "^4.8.0",
        "@nestjs/typeorm": "^7.1.5",
        "@types/lodash": "^4.14.168",
        "aws-sdk": "^2.892.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "faker": "^5.5.3",
        "file-type": "^16.3.0",
        "fs": "^0.0.1-security",
        "helmet": "^4.5.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.30",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "mysql": "^2.18.1",
        "nestjs-i18n": "^8.1.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "rxjs": "^6.6.7",
        "shortid": "^2.2.16",
        "source-map-support": "^0.5.19",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.32",
        "typeorm-seeding": "^1.6.1",
        "typeorm-transactional-cls-hooked": "^0.1.20",
        "typescript": "^4.2.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^7.6.15",
        "@types/bcrypt": "^3.0.1",
        "@types/compression": "^1.7.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@types/faker": "^5.5.4",
        "@types/file-type": "^10.9.1",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^26.0.22",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/mime-types": "^2.1.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.41",
        "@types/passport-jwt": "^3.0.5",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "clean-webpack-plugin": "^3.0.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "gh-pages": "^3.1.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "~10.5.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.0.5",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.1.0",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.3.0",
        "vuepress": "^1.8.2",
        "webpack": "^5.35.1",
        "webpack-cli": "^4.6.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ]
    },
    "_moduleAliases": {
        "@/src": "dist",
        "@/common": "dist/common",
        "@/database": "dist/database",
        "@/decorators": "dist/decorators",
        "@/exceptions": "dist/exceptions",
        "@/filters": "dist/filters",
        "@/guards": "dist/guards",
        "@/interceptors": "dist/interceptors",
        "@/interfaces": "dist/interfaces",
        "@/middlewares": "dist/middlewares",
        "@/modules": "dist/modules",
        "@/providers": "dist/providers",
        "@/shared": "dist/shared"
    }
}
